{
  "name": "@tulpar-express/server",
  "version": "0.1.0",
  "description": "Tulpar Express Server",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node src/main.ts",
    "build": "nest build",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/Migration",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/Migration -d ./src/config/typeorm.config.ts",
    "migration:run": "npm run build && npx typeorm migration:run -d dist/config/typeorm.config.js",
    "migration:revert": "npm run build && npx typeorm migration:revert -d dist/config/typeorm.config.js",
    "schema:drop": "npm run build && npx typeorm schema:drop -d dist/config/typeorm.config.js",
    "start:dev": "nodemon --exec ts-node src/main.ts",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.13",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/typeorm": "^10.0.2",
    "@supabase/supabase-js": "^2.39.3",
    "@types/swagger-ui-express": "^4.1.7",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.0.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
